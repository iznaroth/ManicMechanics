{
  "multipart": [
    {
      "apply": { "model": "industrizer:block/bundle_core"}
    },

    {   "when": { "north": "true" },
      "apply": { "model": "industrizer:block/bundle_north"}
    },

    {   "when": { "east": "true" },
      "apply": { "model": "industrizer:block/bundle_east" }
    },

    {   "when": { "south": "true" },
      "apply": { "model": "industrizer:block/bundle_south"}
    },

    {   "when": { "west": "true" },
      "apply": { "model": "industrizer:block/bundle_west"}
    },

    {   "when": { "up": "true" },
      "apply": { "model": "industrizer:block/bundle_up"}
    },

    {   "when": { "down": "true" },
      "apply": { "model": "industrizer:block/bundle_down"}
    }
  ]
}